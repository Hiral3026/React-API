extends layout

block main-content
  h1.page-title Hello! Here's the list of 15 Popular Shows!
  table
    thead
      tr
        th Show Title
        th Year
    tbody
      each show in showList
        tr
          td
            a(href=`https://imdb.com/title/${show.ids.imdb}`) #{show.title}
          td #{show.year}
